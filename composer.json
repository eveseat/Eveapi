{
  "name": "eveseat/eveapi",
  "description": "SeAT EVE API Integration",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Leon Jacobs",
      "email": "leonja511@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seat\\Eveapi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seat\\Eveapi\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "laravel/framework": "^6.2",
    "eveseat/eseye": "^2.3.0",
    "eveseat/services": "^4.1",
    "guzzlehttp/guzzle": "^6.3",
    "doctrine/dbal": "^2.9",
    "softcreatr/jsonpath": "^0.7",
    "ext-json": "*",
    "ext-redis": "*"
  },
  "require-dev": {
    "josiasmontag/laravel-redis-mock": "^1.0",
    "orchestra/testbench": "^4.0",
    "orchestra/database": "^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\Eveapi\\EveapiServiceProvider"
      ]
    }
  }
}
