{
  "name": "eveseat/eveapi",
  "description": "SeAT EVE API Integration",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Leon Jacobs",
      "email": "leonja511@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seat\\Eveapi\\": "src/",
      "Seat\\Eveapi\\Database\\Seeders\\": "src/database/seeders/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "maatwebsite/excel": "^3.1",
    "eveseat/eseye": "3.x-dev",
    "eveseat/services": "5.0.x-dev",
    "guzzlehttp/guzzle": "^7.0",
    "doctrine/dbal": "^3.0",
    "psr/simple-cache": "^1.0|^2.0",
    "psr/log": "^1.0|^2.0|^3.0",
    "softcreatr/jsonpath": "^0.8",
    "zircote/swagger-php": "^4.0",
    "ext-json": "*",
    "ext-posix": "*",
    "ext-redis": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seat\\Eveapi\\EseyeServiceProvider",
        "Seat\\Eveapi\\EveapiServiceProvider"
      ]
    }
  }
